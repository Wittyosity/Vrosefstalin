extern stall* marketplace(int num);

int main(){

stall *market[3];

int array[3];
int v2=1;
array[0]=v2 = rand() % 5  + 1;

while (v2==array[0])
{
	v2= rand() % 5 + 1;
	array[1]=v2;
}
while (v2==array[0] || v2==array[1])
{
	v2= rand() % 5 + 1;
	array[2]=v2;
}

for (int i=0;i<3;i++)
{
	market[i]=new stall();
	market[i]=marketplace(array[i]);
	//cout << array[i] << endl;
}
